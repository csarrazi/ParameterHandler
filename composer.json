{
    "name": "csa/composer-parameter-handler",
    "description": "Composer script throwing an error if your parameter file is different from the dist",
    "keywords": ["parameters management"],
    "homepage": "https://github.com/csarrazi/ParameterHandler",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        },
        {
            "name": "Charles Sarrazin",
            "email": "charles@sarraz.in"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/yaml": "~2.0"
    },
    "require-dev": {
        "composer/composer": "1.0.*@dev",
        "phpspec/prophecy-phpunit": "~1.0",
        "symfony/filesystem": "~2.2"
    },
    "autoload": {
        "psr-4": { "Csa\\ParameterHandler\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
